#Erro semantico: Atribuindo int com float

:DEC
parametro:INT
fatorial:INT

:PROG
# Calcula o fatorial de um nÃºmero inteiro
LER parametro
fatorial := parametro
SE parametro == 1 ENTAO fatorial := 1
ENQTO parametro > 1
 INI
  fatorial := fatorial * (parametro - 1)
  parametro := parametro - 1.2
 FIM
IMPRIMIR fatorial
IMPRIMIR "Oi"
